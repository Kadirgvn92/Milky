
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_DefaultHeadPartial")

<body>
    <!-- Spinner Start -->
    <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-border text-primary" role="status" style="width: 3rem; height: 3rem;"></div>
    </div>
    <!-- Spinner End -->


    <!-- Topbar Start -->
    @await Component.InvokeAsync("_DefaultTopbarPartial")
    <!-- Topbar End -->


    <!-- Navbar Start -->
    @await Component.InvokeAsync("_DefaultNavbarPartial")
    <!-- Navbar End -->


    <!-- Carousel Start -->
    @await Component.InvokeAsync("_DefaultCarouselPartial")
    <!-- Carousel End -->


    <!-- About Start -->
    @await Component.InvokeAsync("_DefaultAboutPartial")
    <!-- About End -->


    <!-- Features Start -->
    @await Component.InvokeAsync("_DefaultFeaturePartial")
    <!-- Features End -->


    <!-- Banner Start -->
    @await Component.InvokeAsync("_DefaultBannerPartial")
    <!-- Banner End -->


    <!-- Service Start -->
    @await Component.InvokeAsync("_DefaultServicePartial")
    <!-- Service End --> 


    <!-- Gallery Start -->
    @await Component.InvokeAsync("_DefaultGalleryPartial")
    <!-- Gallery End -->


    <!-- Product Start -->
    @await Component.InvokeAsync("_DefaultProductPartial")
    <!-- Product End -->


    <!-- Team Start -->
    @await Component.InvokeAsync("_DefaultTeamPartial")
    <!-- Team End -->


    <!-- Testimonial Start -->
    @await Component.InvokeAsync("_DefaultTestimonialPartial")

    <!-- Testimonial End -->


    <!-- Footer Start -->
    @await Component.InvokeAsync("_DefaultFooterPartial")
    <!-- Footer End -->


    <!-- Copyright Start -->
    <div class="container-fluid bg-secondary text-body copyright py-4">
        <div class="container">
            <div class="row">
                <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
                    &copy; <a class="fw-semi-bold" href="#">Milky</a>,Tüm Hakları Saklıdır.
                </div>
                @* <div class="col-md-6 text-center text-md-end">
                    <!--/*** This template is free as long as you keep the footer author’s credit link/attribution link/backlink. If you'd like to use the template without the footer author’s credit link/attribution link/backlink, you can purchase the Credit Removal License from "https://htmlcodex.com/credit-removal". Thank you for your support. ***/-->
                    Designed By <a class="fw-semi-bold" href="https://htmlcodex.com">HTML Codex</a>
                </div> *@
            </div>
        </div>
    </div>
    <!-- Copyright End -->


    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square rounded-circle back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/dairy/lib/wow/wow.min.js"></script>
    <script src="/dairy/lib/easing/easing.min.js"></script>
    <script src="/dairy/lib/waypoints/waypoints.min.js"></script>
    <script src="/dairy/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="/dairy/lib/counterup/counterup.min.js"></script>
    <script src="/dairy/lib/parallax/parallax.min.js"></script>
    <script src="/dairy/lib/lightbox/js/lightbox.min.js"></script>

    <!-- Template Javascript -->
    <script src="/dairy/js/main.js"></script>
</body>

</html>