
@{
    ViewData["Title"] = "Index";

    ViewData["PageTitle"] = "Ürünler";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

<div class="container-fluid page-header py-5 mb-5 wow fadeIn" data-wow-delay="0.1s" style="visibility: visible; animation-delay: 0.1s; animation-name: fadeIn;">
    <div class="container text-center py-5">
        <h1 class="display-3 text-white mb-4  slideInDown">@ViewData["PageTitle"]</h1>
        <nav aria-label="breadcrumb animated slideInDown">
            <ol class="breadcrumb justify-content-center mb-0">
                <li class="breadcrumb-item"><a href="#">Ana Sayfa</a></li>
                <li class="breadcrumb-item"><a href="#">Sayfalar</a></li>
                <li class="breadcrumb-item active" aria-current="page">@ViewData["PageTitle"]</li>
            </ol>
        </nav>
    </div>
</div>



<!-- Product Start -->
@await Component.InvokeAsync("_DefaultProductPartial")
<!-- Product End -->
<!-- About Start -->
@await Component.InvokeAsync("_DefaultAboutPartial")
<!-- About End -->